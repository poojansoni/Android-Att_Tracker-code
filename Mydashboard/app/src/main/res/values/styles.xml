<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="Toolbar.TitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">45sp</item>
    </style>
    <style name="ShapeAppearanceOverlay.Nav" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">25%</item>
        <item name="cornerSizeBottomRight">25%</item>
        <item name="cornerSizeBottomLeft">25%</item>
        <item name="cornerSizeTopLeft">25%</item>
        <item name="android:elevation">23dp</item>
        <item name="android:padding">25dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:fontFamily">@font/abeezee</item>


    </style>
</resources>
